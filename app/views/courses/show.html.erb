<h1>These student are registered for <strong> <i><%=@course.name %></i></strong> </h1>
<table class="table table-dark">
  <thead>
  <tr>
    <th scope="col">ID</th>
    <th scope="col">Name</th>
    <th scope="col">Roll</th>
  </tr>
  </thead>
  <tbody>
  <% @students.each do |student|%>
    <tr>
      <th scope="row"><%=student.id %></th>
      <td><%= student.name  %></td>
      <td><%= student.roll %></td>
    </tr>
    </tbody>
  <%end %>
  </table>